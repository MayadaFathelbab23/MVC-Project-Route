@model RoleViewModel
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form asp-action="Delete">
    <div class="form-group">
        <label class="col-form-label" asp-for="Id"></label>
        <input asp-for="Id" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="RoleName"></label>
        <input asp-for="RoleName" class="form-control" readonly />
    </div>

    <input type="submit" class="btn btn-secondary" value="Delete" />
    <a asp-action="Index" asp-controller="Role" class="btn btn-">Back To List</a>
</form>

